{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "src"
  },
  "exclude": [
    "lib",
    "dist",
    "misc",
    "public",
    "node_modules",
    "src/modules/**/*",
    "src/templates/helpers/*",
