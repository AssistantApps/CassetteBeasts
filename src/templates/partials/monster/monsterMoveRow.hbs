<tr class="move no-padding">
  <td class="star ta-center">{{source}}</td>
  <td class="type">
    <a href="/{{@root.langCode}}{{@root.routes.moves}}/{{encodeuri this.id}}.html" title="{{this.name_localised}}">
      {{#if this.elemental_types_elements.length}}

      {{#each this.elemental_types_elements}}
      <img src="{{resimage this.icon.path}}" class="move-image" alt="image of {{this.name_localised}}"
        draggable="false">
      {{/each}}

      {{else}}

      <img src="/assets/img/typeless.png" class="move-image" title="Typeless" alt="image of typeless" draggable="false">

      {{/if}}
    </a>
  </td>
  <td class="name">
    <a href="/{{@root.langCode}}{{@root.routes.moves}}/{{encodeuri this.id}}.html" title="{{this.name_localised}}">
      {{this.name_localised}}
    </a>
  </td>
  <td class="category">
    <a href="/{{@root.langCode}}{{@root.routes.moves}}/{{encodeuri this.id}}.html" title="{{this.name_localised}}">
      {{this.category_name_localised}}
    </a>
  </td>
  <td class="power">
    <a href="/{{@root.langCode}}{{@root.routes.moves}}/{{encodeuri this.id}}.html" title="{{this.name_localised}}">
      {{#if this.power}}{{this.power}}{{else}}-{{/if}}
    </a>
  </td>
  <td class="accuracy">
    <a href="/{{@root.langCode}}{{@root.routes.moves}}/{{encodeuri this.id}}.html" title="{{this.name_localised}}">
      {{this.accuracy}}%
    </a>
  </td>
  <td class="ap">
    <a href="/{{@root.langCode}}{{@root.routes.moves}}/{{encodeuri this.id}}.html" title="{{this.name_localised}}">
      {{this.cost}}
    </a>
  </td>
</tr>