{
"name": "{{{meta.appName}}}",
"short_name": "{{meta.title}}",
"description": "{{meta.description}}",
"lang": "en-US",
"scope": "/",
"start_url": "/?standalone",
"display": "standalone",
"icons": [
{{#each images.manifest as |img index|}}
{{#if @index}},{{/if}}
{
"src": "{{rootUrl}}{{img.path}}",
"sizes": "{{img.size}}",
"type": "image/png"
}
{{/each}}
],
"background_color": "#1b1f22",
"theme_color": "{{theme.colour}}"
}